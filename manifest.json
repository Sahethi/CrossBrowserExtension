{
    "manifest_version": 2,
    "name": "CBX",
    "description": "This extension is to detect multiple attacks like XSS, iFrame Detection.",
    "version": "1.0",
  
    "browser_action": {
      "default_icon": "new_icon.png",
      "default_popup" : "popup.html"
    },
    "content_scripts": [
    {
      "matches": ["*://*/*"],
      "run_at": "document_start",
      "js": ["my_script.js"],
      "all_frames": true
    }
    ],

    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      },

    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "<all_urls>",
      "notifications",
      "storage"
    ],
    "background": {
      "scripts": ["background.js","phishing.js"],
      "persistent": true
    }
  }
  